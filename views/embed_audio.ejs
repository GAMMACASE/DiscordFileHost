<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<meta property="og:site_name" content="DiscordFileHost">
	<meta property="og:type" content="music.song">
	<meta property='og:audio' content=<%= fileURL %>>
	<meta property="og:audio:secure_url" content=<%= fileURL %>>
	<meta property='og:audio:type' content='audio/wav'>
	
	<meta property="og:url" content=<%= fileURL %>>
	<meta property="og:description" content="<%= fileName %> âˆ™ <%= fileSize %>">
	<meta property="og:title" content="Download">
	<meta property="og:image" content="<%= frontURL %>/icons/file.png">
	
	<meta name="theme-color" content="#596AED">
	<script>
		window.location.replace("<%= fileURL %>");
		
		setTimeout(() => {
			window.close();
		}, 3000);
	</script>
</head>
<body>
</body>
</html>