{
  "name": "discordfilehost",
  "version": "1.0.0",
  "description": "File hosting app with a simple setup",
  "main": "./dist/index.js",
  "scripts": {
    "server": "node -r tsconfig-paths/register .",
    "dev-server": "nodemon --enable-source-maps --watch dist/** -r tsconfig-paths/register .",
    "lint": "eslint . --ext .ts",
    "package": "webpack --config webpack.prod.js",
    "dev-package": "webpack serve --config webpack.dev.js",
    "build": "tsc",
    "build-watch": "tsc --watch"
