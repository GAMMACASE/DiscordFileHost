    "@types/express": "^4.17.13",
    "@types/node": "^18.7.18",
    "@types/passport-discord": "^0.1.6",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "autoprefixer": "^10.4.8",
    "bootstrap": "^5.2.0",
    "clean-css": "^5.3.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.23.0",
